{
  "name": "diagram-tool",
  "version": "0.0.1",
  "description": "A cloud infrastructure diagramming app.",
  "scripts": {
    "build:template": "mkdir dist && do-vue template && node ./build/create_index.js",
    "build:autogen-icon-imports": "node ./src/data/additionalIconsGenerator.js",
    "build:webpack": "webpack --config webpack.prod.js",
    "build": "npm run clean && cross-env BLANK_TEMPLATE=false npm run build:template && npm run build:autogen-icon-imports && npm run build:webpack",
    "test": "eslint ./src/**/*.js",
    "lint-and-fix": "eslint ./src/**/*.js --fix",
    "clean": "rimraf dist",
    "start:__base": "npm run clean && npm run build:template && npm run build:autogen-icon-imports && webpack-dev-server --config webpack.dev.js",
    "start": "cross-env BLANK_TEMPLATE=false npm run start:__base",
    "start:no-do": "cross-env BLANK_TEMPLATE=true npm run start:__base",
    "deploy:spaces:comment": "do-vue comment diagram-tool"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/do-community/diagram-tool.git"
  },
  "browserslist": [
    "last 10 chrome version",
    "last 5 firefox version"
  ],
  "author": "DigitalOcean",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "babel-eslint": "10.1.0",
    "babel-types": "6.26.0",
    "d3": "^4.13.0",
    "do-bulma": "git+https://github.com/do-community/do-bulma.git",
    "eslint": "6.8.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-react": "7.18.3",
    "lodash": "^4.17.19",
    "raven-js": "^2.3.0",
    "react": "^16.13.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "^16.13.1",
    "react-image-gallery": "^1.0.8",
    "react-redux": "^7.2.1",
    "react-redux-form": "^1.16.14",
    "react-router": "^3.2.5",
    "react-router-redux": "^4.0.8",
    "react-svg": "^11.0.39",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.0",
    "rimraf": "^2.7.1",
    "roughjs": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^7.0.2",
    "css-loader": "3.6.0",
    "do-vue": "git+https://github.com/do-community/do-vue.git",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "posthtml": "0.12.0",
    "posthtml-custom-elements": "^1.1.2",
    "posthtml-extend": "0.3.0",
    "prettier": "1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "sass": "^1.26.10",
    "sass-loader": "9.0.2",
    "terser-webpack-plugin": "3.0.7",
    "ts-loader": "8.0.1",
    "typescript": "^3.8.3",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.0.9"
  }
}
