{
  "name": "infragram",
  "version": "0.0.1",
  "description": "A cloud infrastructure diagramming app.",
  "scripts": {
    "build:template": "do-vue template && node ./build/create_index.js",
    "build:parcel": "parcel build ./client/infragram.js --public-url . && parcel build ./client/styles/style.styl --public-url .",
    "build": "npm run clean && npm run build:template && npm run build:parcel && cp ./index.html ./dist/index.html",
    "test": "eslint ./client/**/*.js",
    "lint-and-fix": "eslint ./client/**/*.js --fix",
    "clean": "rimraf dist && rm index.html",
    "start": "BLANK_TEMPLATE=false do-vue template && node ./build/create_index.js && node ./devServer.js",
    "tunnel": "browser-sync start --proxy localhost:7770  --tunnel andy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/do-community/infragram.git"
  },
  "author": "DigitalOcean",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-eslint": "10.0.3",
    "babel-types": "6.26.0",
    "core-js": "^3.6.4",
    "d3": "^4.13.0",
    "do-bulma": "git+https://github.com/do-community/do-bulma.git",
    "eslint": "6.8.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-react": "7.18.3",
    "lodash": "^4.17.15",
    "raven-js": "^2.3.0",
    "react": "^16.12.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-redux-form": "^1.16.14",
    "react-router": "^3.2.5",
    "react-router-redux": "^4.0.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.0",
    "rimraf": "^2.7.1",
    "roughjs": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "do-vue": "git+https://github.com/do-community/do-vue.git",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "mime-types": "2.1.26",
    "mocha": "^5.2.0",
    "parcel-bundler": "1.12.4",
    "posthtml": "0.12.0",
    "posthtml-custom-elements": "^1.1.2",
    "posthtml-extend": "0.3.0",
    "prettier": "1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "webpack-cli": "3.3.10",
    "express": "^4.17.1",
    "express-ws": "^4.0.0"
  }
}
